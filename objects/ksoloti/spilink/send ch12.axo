<objdefs appVersion="1.0.12">
   <obj.normal id="send ch12" uuid="51ef8fea-27a3-42d2-a9c1-27548caaae53">
      <sDescription>Send/receive audio between linked boards.
NOTE: X/Y placement of spilink objects within your patch is important due to the tight timing. Try to place the spilink objects at the top of your canvas if you encounter noise or aliasing.</sDescription>
      <author>Johannes Taelman, sebiik</author>
      <license>GPL</license>
      <inlets>
         <frac32buffer name="ch1" description="channel 1"/>
         <frac32buffer name="ch2" description="channel 2"/>
      </inlets>
      <outlets/>
      <displays/>
      <params/>
      <attribs/>
      <code.krate><![CDATA[int i;
for(i=0; i<BUFSIZE; i++)
{
	spilink_tx_samples->channel[0].samples[i] = inlet_ch1[i];
	spilink_tx_samples->channel[1].samples[i] = inlet_ch2[i];
}]]></code.krate>
   </obj.normal>
</objdefs>