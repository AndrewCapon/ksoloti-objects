<objdefs appVersion="1.0.12">
   <obj.normal id="send 2" uuid="cae1c5ae-6da2-4a7b-918e-bd52fe6033bc">
      <sDescription>Send/receive audio between linked boards.
NOTE: X/Y placement of spilink objects within your patch is important due to the tight timing. If you encounter noise or aliasing, try to place the spilink objects at the top of your canvas and the send objects higher than the receive objects.</sDescription>
      <author>Johannes Taelman, sebiik</author>
      <license>GPL</license>
      <inlets>
         <frac32buffer name="inA" description="input A"/>
         <frac32buffer name="inB" description="input B"/>
      </inlets>
      <outlets/>
      <displays/>
      <params/>
      <attribs>
         <combo name="channels">
            <MenuEntries>
               <string>1 | 2</string>
               <string>3 | 4</string>
               <string>5 | 6</string>
            </MenuEntries>
            <CEntries>
               <string>0</string>
               <string>2</string>
               <string>4</string>
            </CEntries>
         </combo>
      </attribs>
      <code.krate><![CDATA[int i;
for(i=0; i<BUFSIZE; i++)
{
	spilink_tx_samples->channel[attr_channels].samples[i] = inlet_inA[i];
	spilink_tx_samples->channel[attr_channels+1].samples[i] = inlet_inB[i];
}]]></code.krate>
   </obj.normal>
</objdefs>